    "vue": "^3.2.45",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@testing-library/vue": "^6.6.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-vue": "^9.9.0",
    "happy-dom": "^8.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.5.1",
    "typescript": "^4.4.4",
    "vite": "^4.0.0",
    "vitest": "^0.29.2",
